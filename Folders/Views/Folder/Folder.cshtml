@model Folder[]


<h2 class="text-center">./@ViewData["Folder"]/</h2>
<div class="list-group">
    @foreach(var item in Model){
        <a class="list-group-item list-group-item-action" 
            asp-controller="Folder" 
            asp-action="Folder"
            asp-route-id="@item.FolderId">
            @item.Name
        </a>
    }
</div>